
package javaapplication6;

import javax.swing.JOptionPane;


public class Senha {

    
    public static void main(String[] args) {
        
        String senha = JOptionPane.showInputDialog("Criar senha: ").toUpperCase();
        String senhaDigitada = "";
        
        do {
            senhaDigitada = JOptionPane.showInputDialog("Digite a senha").toUpperCase();
            
        }while (!senhaDigitada.equals(senha));
        
        JOptionPane.showMessageDialog(null, "Seja bem vindo!");
        JOptionPane.showMessageDialog(null, "Senha:" +senha+"\nSenhaDigitada"+ senhaDigitada);
    }
}
